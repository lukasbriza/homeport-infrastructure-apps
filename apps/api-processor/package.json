{
  "name": "@lukasbriza/api-processor",
  "version": "1.0.0",
  "private": true,
  "description": "Internal jenkins application for processing api requests to portainer and passbolt",
  "main": "index.js",
  "type": "module",
  "scripts": {
    "dev": "nodemon",
    "start": "node ./build/server.js",
    "start:production": "pm2 start ./build/server.js --name api-processor",
    "stop:production": "pm2 stop api-processor & pm2 delete api-processor",
    "lint": "eslint --cache .",
    "lint:fix": "pnpm run lint --fix",
    "build": "rollup -c"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@lukasbriza/eslint-config": "workspace:*",
    "@lukasbriza/ts-config": "workspace:*",
    "axios": "1.7.7",
    "cookie-parser": "1.4.7",
    "dotenv": "16.4.5",
    "express": "4.21.1",
    "openpgp": "5.11.2",
    "pm2": "5.4.2"
  },
  "devDependencies": {
    "@rollup/plugin-commonjs": "28.0.1",
    "@rollup/plugin-json": "6.1.0",
    "@rollup/plugin-node-resolve": "15.3.0",
    "@rollup/plugin-terser": "0.4.4",
    "@rollup/plugin-typescript": "12.1.1",
    "@types/cookie-parser": "1.4.7",
    "@types/express": "5.0.0",
    "@types/node": "20.14.9",
    "nodemon": "3.1.7",
    "rollup": "4.24.4",
    "ts-node": "10.9.2",
    "typescript": "5.5.3"
  }
}